  "bugs": {
    "url": "https://github.com/lallaria/Chatipelago/issues"
  },
  "homepage": "https://github.com/lallaria/Chatipelago#readme",
  "devDependencies": {
    "eslint": "^8.57.1",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "customConfig/**/*",
      "icon.png",
      "server.js",
      "admin-server.js",
      "bot-get.js",
      "webhook-put.js",
      "config.js",
      "apWorldSettings.js",
      "archipelagoHelper.js",
