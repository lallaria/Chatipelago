{
  "name": "chatipelago",
  "version": "1.1.10",
  "description": "Chatipelago",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "node app.js",
    "admin": "node admin-server.js",
    "client": "node server.js",
    "build:bundle": "node scripts/build-bundle.js",
    "build:sea-config": "node scripts/generate-sea-config.js",
    "build:sea": "npm run build:sea-config && node --experimental-sea-config sea-config.json",
    "build": "npm run build:bundle && npm run build:sea && node scripts/inject-sea.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lallaria/Chatipelago.git"
  },
  "author": {
    "name": "DelilahIsDidi Dranzior LMarioza"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lallaria/Chatipelago/issues"
  },
  "homepage": "https://github.com/lallaria/Chatipelago#readme",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "postject": "^1.0.0-alpha.6"
  },
  "eslintConfig": {
    "env": {
      "es2022": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@streamerbot/client": "^1.12.2",
    "archipelago.js": "^2.0.4",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
